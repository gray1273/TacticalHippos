// Base methods to create and control cards

class DeckKernel {

	var baseCards, inUseCards, usedCards;


	constructor(){
		reset();
	}
	
	reset(){
		baseCards = [];
		inUseCards = [];
		usedCards = [];
		for(let i = 0; i < 3; i++){
			for(let j = 0; j < 3; j++){
				for(let k = 0; k < 3; k++){
					for(let l = 0; l < 3; l++){
						baseCards.push(new Card(i, j, k, l));
					}
				}
			}
		}
		
		shuffle();
		get(12);
	}


	shuffle() {

	}

}
