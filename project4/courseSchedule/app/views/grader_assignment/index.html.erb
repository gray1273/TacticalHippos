<!DOCTYPE html>
<html>
    <head>
        <title>Assign Graders</title>
    </head>
    <body>
        <table>
            <tr>
                <th>Course</th>
                <th>Graders Assigned</th>
                <th>Edit</th>
            </tr>
            <% @data.each do |course| %>
                <tr>
                    <td><p><%= course.course_name %></p></td>
                    <td>
                        <% course.sections.each do |section| %>
                            <% puts section.methods %>
                        <% end %>
                    </td>
                    <td>Edit</td>
                </tr>
            <% end %>
        </table>
    </body>
</html>
