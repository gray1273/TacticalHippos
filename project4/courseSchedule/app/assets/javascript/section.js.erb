function toggleGraderSection(id){
	if(document.getElementById("section-"+id+"-checkbox").checked){
	    const div = document.createElement('div');
	    div.id = "section-"+id+"-button"
	    div.innerHTML = 'CHECKED';
	    document.getElementById("section-"+id).appendChild(div);
  	} else {
    	document.getElementById("section-"+id).removeChild(document.getElementById("section-"+id+"-button"));
  	}
}
